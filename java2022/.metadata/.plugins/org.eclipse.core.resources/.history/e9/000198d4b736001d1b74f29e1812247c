package conditionals;

public class main {

	public static void main(String[] args) {
		int sayi = 24;
		
		if (sayi<20) {
			System.out.println("Sayı 20 den küçüktür.");
		}else {
			System.out.println("Sayı 20 den küçük değildir.");
		}
		
		
		
		if (sayi<15) {
			System.out.println("Sayi 20 den büyüktür.");
		} 
		
	}

}
