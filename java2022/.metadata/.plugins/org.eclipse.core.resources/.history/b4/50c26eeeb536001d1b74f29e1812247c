package veriables;

public class main {

	//reusability tek sefer yaz ve kullan 
	
	public static void main(String[] args) {
		int ogrenciSayisi =12;
		String mesaj = "Öğrenci sayım : ";
		
		System.out.println(mesaj + ogrenciSayisi);
		System.out.println(mesaj + ogrenciSayisi);
		System.out.println(mesaj + ogrenciSayisi);
		System.out.println(mesaj + ogrenciSayisi);
	}

}
