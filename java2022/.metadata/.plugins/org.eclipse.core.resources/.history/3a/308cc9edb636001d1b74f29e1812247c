package datatypes;

public class main {

	public static void main(String[] args) {
		
		int sayi = 12;
		double sayi2 = 12.5;
		byte sayi3 = -128;
		
		char karakter = 'B';
		boolean yanlis= false;
		
		System.out.println(sayi3);
		System.out.println(karakter);
		System.out.println(yanlis);
		
		
	}

}
